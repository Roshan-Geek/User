{
    "workbench.iconTheme": "material-icon-theme",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "prettier.tabWidth": 4,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,
    "[html]": {
        "editor.defaultFormatter": "sissel.shopify-liquid"
    },

    "liquid.format": true,

    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },

    "[css]": {},

    "[javascript]": {},

    "[javascriptReact]": {},

    "[sass]": {},

    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },

    "liveServer.settings.donotShowInfoMsg": true,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 24.5,
    "window.restoreFullscreen": true,
    "explorer.incrementalNaming": "smart",
    "workbench.editor.untitled.hint": "hidden",
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.startupEditor": "none",
    "material-ui-snippets.showNotesOnStartup": false,
    "git.autofetch": true,
    "editor.foldingMaximumRegions": 65000,
    "workbench.colorCustomizations": {
        "terminal.background": "#212121",
        "terminal.foreground": "#CBD6E2",
        "terminalCursor.background": "#CBD6E2",
        "terminalCursor.foreground": "#CBD6E2",
        "terminal.ansiBlack": "#0B1C2C",
        "terminal.ansiBlue": "#8B56BF",
        "terminal.ansiBrightBlack": "#627E99",
        "terminal.ansiBrightBlue": "#8B56BF",
        "terminal.ansiBrightCyan": "#568BBF",
        "terminal.ansiBrightGreen": "#56BF8B",
        "terminal.ansiBrightMagenta": "#BF568B",
        "terminal.ansiBrightRed": "#BF8B56",
        "terminal.ansiBrightWhite": "#F7F9FB",
        "terminal.ansiBrightYellow": "#8BBF56",
        "terminal.ansiCyan": "#568BBF",
        "terminal.ansiGreen": "#56BF8B",
        "terminal.ansiMagenta": "#BF568B",
        "terminal.ansiRed": "#BF8B56",
        "terminal.ansiWhite": "#CBD6E2",
        "terminal.ansiYellow": "#8BBF56"
    },
    "terminal.integrated.cursorBlinking": true,
    "importCost.margin": 0.7,
    "importCost.fontStyle": "oblique",
    "reactSnippets.settings.prettierEnabled": true,
    "window.confirmBeforeClose": "keyboardOnly",
    "terminal.explorerKind": "external",
    "terminal.integrated.cursorWidth": 2,
    "cSpell.customDictionaries": {
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "~/.cspell/custom-dictionary-user.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "editor.renderWhitespace": "all",
    "scm.diffDecorationsIgnoreTrimWhitespace": "true",
    "bracketPairColorizer.timeOut": 300,
    "bracketPairColorizer.activeScopeCSS": ["borderStyle : dotted", "borderWidth : 1px"],
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "bracketPairColorizer.rulerPosition": "Full",
    "bracketPairColorizer.highlightActiveScope": true,
    "prettier.jsxBracketSameLine": true,
    "bracketPairColorizer.showBracketsInGutter": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "window.menuBarVisibility": "compact",
    "window.dialogStyle": "custom",
    "files.simpleDialog.enable": true,
    "git-autoconfig.configList": [
        {
            "user.email": "roshangeek435@gmail.com",
            "user.name": "roshangeek435"
        },
        {
            "user.email": "mauliksuvagiya@gmail.com",
            "user.name": "mauliksuvagiya"
        },
        {
            "user.email": "swapnilgeek435@gmail.com",
            "user.name": "Swaps300"
        }
    ],
    "gitlens.defaultDateFormat": "DD-MM-YYYY",
    "gitlens.codeLens.scopes": ["document", "containers"],
    "gitlens.statusBar.alignment": "left",
    "prettier.printWidth": 100,
    "yarn.enableTouchbar": true,
    "git.confirmSync": false,
    "liveSassCompile.settings.generateMap": false,
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "~/../css/"
        },
        {
            "extensionName": ".min.css",
            "format": "compressed",
            "savePath": "~/../css/"
        }
    ],
    "liveSassCompile.settings.autoprefix": ["> 1%", "last 2 versions"],
    "bracketPairColorizer.depreciation-notice": false,
    "oneDarkPro.vivid": true,
    "files.autoSave": "afterDelay",
    "editor.guides.bracketPairs": true
}
