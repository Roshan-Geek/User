        // "terminal.ansiYellow":"#8BBF56"

        "terminal.background": "#212121",
        "terminal.foreground": "#D0D0D0",
        "terminalCursor.background": "#D0D0D0",
        "terminalCursor.foreground": "#D0D0D0",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#0066FF",
        "terminal.ansiBrightBlack": "#808080",
        "terminal.ansiBrightBlue": "#0066FF",
        "terminal.ansiBrightCyan": "#00FFFF",
        "terminal.ansiBrightGreen": "#33FF00",
        "terminal.ansiBrightMagenta": "#CC00FF",
        "terminal.ansiBrightRed": "#FF0000",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightYellow": "#FF0099",
        "terminal.ansiCyan": "#00FFFF",
        "terminal.ansiGreen": "#33FF00",
        "terminal.ansiMagenta": "#CC00FF",
        "terminal.ansiRed": "#FF0000",
        "terminal.ansiWhite": "#D0D0D0",
        "terminal.ansiYellow": "#FF0099",

        "[One Dark Pro Darker]": {
            "tab.activeBorderTop": "#FFFFFF"
        }
    },
    "terminal.integrated.cursorBlinking": true,
    "importCost.timeout": 2000,
    "importCost.margin": 0.7,
    "importCost.fontStyle": "oblique",
    "reactSnippets.settings.prettierEnabled": true,
    "window.confirmBeforeClose": "keyboardOnly",
    "terminal.explorerKind": "external",
    "terminal.integrated.cursorWidth": 2,
    "cSpell.customDictionaries": {
        "custom-dictionary-user": {
            "name": "custom-dictionary-user",
            "path": "~/.cspell/custom-dictionary-user.txt",
            "addWords": true,
            "scope": "user"
        }
    },
    "editor.renderWhitespace": "all",
    "scm.diffDecorationsIgnoreTrimWhitespace": "true",
    "bracketPairColorizer.timeOut": 300,
    "bracketPairColorizer.activeScopeCSS": [
        "borderStyle : dotted",
        "borderWidth : 1px",
        "borderColor : {color}; opacity: 0.8"
    ],
    "editor.bracketPairColorization.enabled": true,
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.guides.bracketPairs": true,
    "editor.cursorBlinking": "smooth",
    "editor.cursorSmoothCaretAnimation": true,
    "bracketPairColorizer.rulerPosition": "Full",
    "bracketPairColorizer.highlightActiveScope": true,
    "prettier.jsxBracketSameLine": true,
    "bracketPairColorizer.showBracketsInGutter": true,
    "workbench.colorTheme": "One Dark Pro Darker",
    "window.menuBarVisibility": "compact",
    "window.dialogStyle": "custom",
    "files.simpleDialog.enable": true,
    "git-autoconfig.configList": [
        {
            "user.email": "roshangeek435@gmail.com",
            "user.name": "roshangeek435"
        },
        {
            "user.email": "mauliksuvagiya@gmail.com",
            "user.name": "mauliksuvagiya"
        }
    ],
    "gitlens.defaultDateFormat": "DD-MM-YYYY",
    "gitlens.codeLens.scopes": ["document", "containers"],
    "gitlens.statusBar.alignment": "left",
    "prettier.printWidth": 100,
    "yarn.enableTouchbar": true,
    "git.confirmSync": false,
    "liveSassCompile.settings.generateMap": false,
    "liveSassCompile.settings.formats": [
        {
            "format": "expanded",
            "extensionName": ".css",
            "savePath": "~/../css/"
        },
        {
            "extensionName": ".min.css",
            "format": "compressed",
            "savePath": "~/../css/"
        }
    ],
    "liveSassCompile.settings.autoprefix": ["> 1%", "last 2 versions"],
    "bracketPairColorizer.depreciation-notice": false,
    "oneDarkPro.vivid": true
}
