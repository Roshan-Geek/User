{
    "workbench.iconTheme": "material-icon-theme",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 4,
    "prettier.tabWidth": 4,
    "editor.formatOnSave": true,
    "editor.insertSpaces": true,
    "editor.detectIndentation": false,

    "[php]": {
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
    },

    "[html]": {},

    "[css]": {},

    "[javascript]": {},

    "[javascriptReact]": {},

    "[sass]": {},

    "emmet.includeLanguages": {
        "javascript": "javascriptreact"
    },
    // "prettier.documentSelectors": ["sass", "html", "php", "css", "javascript"],
    "liveServer.settings.donotShowInfoMsg": true,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "never",
    "security.workspace.trust.enabled": false,
    "editor.fontFamily": "Jetbrains Mono",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.lineHeight": 24.5,
    "window.restoreFullscreen": true,
    "explorer.incrementalNaming": "smart",
    "workbench.editor.untitled.hint": "hidden",
    "liquid.format": true,
    "explorer.confirmDelete": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "workbench.startupEditor": "none",
    "material-ui-snippets.showNotesOnStartup": false,
    "git.autofetch": true,
    "editor.foldingMaximumRegions": 65000,
    "workbench.colorCustomizations": {
        // "terminal.background":"#212121",
        // "terminal.foreground":"#CBD6E2",
        // "terminalCursor.background":"#CBD6E2",
        // "terminalCursor.foreground":"#CBD6E2",
